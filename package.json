{
  "name": "tree-sitter-xquery",
  "version": "0.0.1",
  "author": "Grant Mackenzie <grantmacken@gmail.com>",
  "license": "MIT",
  "keywords": [
    "parser",
    "xquery"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/grantmacken/tree-sitter-xquery.git"
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse",
    "highlight": "tree-sitter query --captures queries/highlights.scm",
    "build": "yarn generate && node-gyp build",
    "build-wasm": "tree-sitter build-wasm",
    "web": "tree-sitter web-ui"
  },
  "dependencies": {
    "nan": "2.14.2",
    "node-gyp": "^9.0.0"
  },
  "devDependencies": {
    "prettier": "2.3.2",
    "tree-sitter-cli": "0.20.6"
  },
  "main": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.xquery",
      "file-types": [
        "xquery"
      ]
    }
  ]
}
