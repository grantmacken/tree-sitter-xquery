{
  "name": "tree-sitter-xquery",
  "description": "XQuery tree-sitter",
  "version": "0.1.1",
  "author": "Grant Mackenzie <grantmacken@gmail.com>",
  "license": "MIT",
  "keywords": [
    "XQuery",
    "treesitter",
    "tree-sitter",
    "parser",
    "grammar",
    "tooling",
    "XML",
    "JSON"
  ],
  "homepage": "https://grantmacken.github.io/tree-sitter-xquery",
  "repository": {
    "type": "git",
    "url": "https://github.com/grantmacken/tree-sitter-xquery.git"
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse",
    "highlight": "tree-sitter query --captures queries/highlights.scm",
    "build": "yarn generate && node-gyp build",
    "build-wasm": "tree-sitter build-wasm",
    "web": "tree-sitter web-ui",
    "tags": "tree-sitter tags" 
  },
  "dependencies": {
    "nan": "^2.17.0",
    "node-gyp": "^9.0.0"
  },
  "devDependencies": {
    "prettier": "^2.8.7",
    "tree-sitter-cli": "^0.20.8",
    "web-tree-sitter": "^0.20.5"
  },
  "main": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.xquery",
      "file-types": [
        "xquery", "xqm", "xq", "xql"
      ]
    }
  ]
}
